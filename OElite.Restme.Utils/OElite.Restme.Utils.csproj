<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Restme.Utils (RESTme) is the common library for Restme</Description>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>OElite.Restme.Utils</AssemblyName>
    <PackageTags>RESTme</PackageTags>
    <RepositoryUrl>https://github.com/oelite/RESTme</RepositoryUrl>
    <DebugType Condition="'$(OS)' != 'Windows_NT'">portable</DebugType>
    <VersionPrefix>2.0.1-dev5</VersionPrefix>
    <Authors>OElite Limited</Authors>
    <Owners>OElite Limited</Owners>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="System.Reflection.TypeExtensions" Version="4.4.0" />
    <PackageReference Include="System.Security.Cryptography.Algorithms" Version="4.3.0">
    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Microsoft.Extensions.Logging.Abstractions, Version=2.0.0.0, Culture=neutral, PublicKeyToken=adb9793829ddae60">
      <HintPath>..\..\..\..\.nuget\packages\microsoft.extensions.logging.abstractions\2.0.0\lib\netstandard2.0\Microsoft.Extensions.Logging.Abstractions.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>